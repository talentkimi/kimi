package core.http.browser;

/**
 * An Internet Explorer Factory.
 */
class InternetExplorerFactory extends HttpBrowserFactory {

	/** The default browser. */
	private static final InternetExplorer defaultBrowser = new InternetExplorer();

	/**
	 * Creates a new browser.
	 * @return a new browser.
	 */
	public HttpBrowser getBrowser() {
		return defaultBrowser;
	}

}