package core.xml;

/**
 * An XML Container.
 */
public interface XmlContainer {

	/**
	 * Returns this object as xml.
	 * @return this object as xml.
	 */
	public Xml toXml() throws XmlException;

	/**
	 * Parses this object from the given xml.
	 * @param xml the xml.
	 */
	public void fromXml(Xml xml) throws XmlException;

}
