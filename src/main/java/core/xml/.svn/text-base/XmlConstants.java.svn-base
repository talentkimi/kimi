package core.xml;

/**
 * A XML Constants.
 */
public interface XmlConstants {

	/** New name. */
	byte NAME_NEW = 0;
	/** Name index. */
	byte NAME_INDEX = 1;
	/** New node. */
	byte NODE_NEW = 2;
	/** Set node value. */
	byte NODE_VALUE = 3;
	/** Node end. */
	byte NODE_END = 4;
	/** New attribute. */
	byte ATTRIBUTE_NEW = 5;
	/** Set attribute value. */
	byte ATTRIBUTE_VALUE = 6;
	/** Attribute end. */
	byte ATTRIBUTE_END = 7;

}
