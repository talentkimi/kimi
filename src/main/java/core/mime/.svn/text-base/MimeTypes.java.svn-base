package core.mime;

/**
 * MIME Types.
 */
public interface MimeTypes {

	/** The JPEG content type. * */
	String IMAGE_JPEG = "image/jpeg";

	String APPLICATION_X_GZIP = "application/x-gzip";
	
	String APPLICATION_OCTET_STREAM = "application/octet-stream";
	
	/** The PLAIN content type. * */
	String TEXT_PLAIN = "text/plain";
	/** The HTML content type. * */
	String TEXT_HTML = "text/html";
	/** The XML content type. * */
	String TEXT_XML = "text/xml";
	/** The URL encoded content type. * */
	String APPLICATION_X_WWW_FORM_URLENCODED = "application/x-www-form-urlencoded";

}