package core.http.exception;

/**
 * An HTTP OK Exception.
 */
public class HttpOkException extends HttpException {

	/**
	 * Creates a new bad request exception.
	 * @param message the message.
	 */
	public HttpOkException(Object message) {
		super(CODE_200, null, message, null);
	}

	/**
	 * Creates a new bad request exception.
	 * @param message the message.
	 */
	public HttpOkException(Object message, String contentType) {
		super(CODE_200, null, message, contentType);
	}

}