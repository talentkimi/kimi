package core.http;

/**
 * A list of general HTTP headers.
 */
public interface HttpMessageHeaderList {

	/** Cache Control. The cache settings (e.g. no-cache). **/
	String HEADER_CACHE_CONTROL = "Cache-Control";

	/** Connection. The connection settings (e.g. close, keep-alive). **/
	String HEADER_CONNECTION = "Connection";

	/** Content Encoding. The content encoding applied over the content type. **/
	String HEADER_CONTENT_ENCODING = "Content-Encoding";
	/** Content Language. The language of the content. **/
	String HEADER_CONTENT_LANGUAGE = "Content-Language";
	/** Content Length. The length of the HTTP content in bytes. **/
	String HEADER_CONTENT_LENGTH = "Content-Length";
	/** Content Location. The separate URI location of the content. **/
	String HEADER_CONTENT_LOCATION = "Content-Location";
	/** Content MD5. The MD5 of the content for integrity checking. **/
	String HEADER_CONTENT_MD5 = "Content-MD5";
	/** Content Type. The MIME type of the content. **/
	String HEADER_CONTENT_TYPE = "Content-Type";

	/** Date. The HTTP date at which the message was generated. **/
	String HEADER_DATE = "Date";

	/** Trailer. The headers included after a chunk-encoded content. **/
	String HEADER_TRAILER = "Trailer";

	/** Transfer Encoding. The encoding of the HTTP content. **/
	String HEADER_TRANSFER_ENCODING = "Transfer-Encoding";

	/** Via. The gateways and proxies involved in the tranfer of the request/response. **/
	String HEADER_VIA = "Via";

	/** Warning. Warnings about the status of the message. **/
	String HEADER_WARNING = "Warning";

}
