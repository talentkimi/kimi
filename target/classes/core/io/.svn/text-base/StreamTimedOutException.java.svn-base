package core.io;


/**
 * Stream Timed Out Exception
 */
final public class StreamTimedOutException extends TimeoutException {

	static final long serialVersionUID = -1020672763563419686L;

	/**
	 * Constructor
	 */
	public StreamTimedOutException() {
		super();
	}

	/**
	 * Constructor
	 * @param message
	 */
	public StreamTimedOutException(String message) {
		super(message);
	}

	/**
	 * Constructor
	 * @param cause
	 */
	public StreamTimedOutException(Throwable cause) {
		super(cause);
	}
}