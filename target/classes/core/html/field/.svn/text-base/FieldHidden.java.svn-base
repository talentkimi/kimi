package core.html.field;

/**
 * A Hidden Field.
 */
public final class FieldHidden extends FieldInput {

	/**
	 * Creates a new input.
	 * @return a new input.
	 */
	protected final FieldInput newInput() {
		return new FieldHidden();
	}

	/**
	 * Retrun hidden.
	 * @return
	 */
	public boolean isHidden() {
		return true;
	}

	public String getType() {
		return "Input";
	}

}