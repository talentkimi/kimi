package core.text;

/**
 * A Selection Exception.
 */
public class SelectionException extends Exception {

	/**
	 * Creates a new selection exception.
	 * @param message the message.
	 */
	public SelectionException() {
	}

	/**
	 * Creates a new selection exception.
	 * @param message the message.
	 */
	public SelectionException(String message) {
		super(message);
	}

}
